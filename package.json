{
  "name": "@inkline/inkline",
  "version": "3.0.8",
  "description": "Inkline is the intuitive UI Components library that gives you a developer-friendly foundation for building high-quality, accessible, and customizable Vue.js 3 Design Systems.",
  "author": "Alex Grozav <alex@grozav.com>",
  "scripts": {
    "dev": "npm run storybook",
    "build": "rimraf lib dist && tsc -d -p tsconfig.json && mjscjs -p tsconfig.json && vite build",
    "postbuild": "ts-node ./scripts/postbuild.ts",
    "lint": "npm run lint:script && npm run lint:style",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint:script": "eslint --ext .vue,.js,.jsx,.ts,.tsx src",
    "lint:style": "stylelint \"src/**/*.scss\"",
    "prerelease": "npm run lint && npm run test && npm run build",
    "release": "np",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "generate:manifest": "ts-node ./scripts/manifest.ts",
    "generate:manifest:sass": "ts-node ./scripts/manifest-sass.ts",
    "storybook": "npm-run-all --parallel storybook:react storybook:vue",
    "storybook:react": "cd storybook/react && npm run dev",
    "storybook:vue": "cd storybook/vue && npm run dev",
    "build:storybook": "build-storybook"
  },
  "main": "inkline.cjs",
  "module": "inkline.mjs",
  "jsdelivr": "inkline.cjs",
  "unpkg": "inkline.cjs",
  "types": "inkline.d.ts",
  "dependencies": {
    "@inkline/paper": "1.4.3",
    "@popperjs/core": "2.11.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/vue": "6.5.0",
    "@types/fs-extra": "9.0.13",
    "@types/node": "17.0.13",
    "@types/react": "17.0.40",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "@vitejs/plugin-vue": "2.1.0",
    "@vue/compiler-sfc": "3.2.29",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.0.0-rc.16",
    "babel-loader": "8.2.3",
    "c8": "7.11.0",
    "comment-parser": "1.3.0",
    "cross-env": "7.0.3",
    "eslint": "8.8.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-vue": "8.4.0",
    "fs-extra": "10.0.0",
    "glob": "7.2.0",
    "html-loader": "3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "19.0.0",
    "miragejs": "0.1.43",
    "mjscjs": "1.0.9",
    "np": "7.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "postcss-preset-env": "7.2.3",
    "qs": "6.10.3",
    "raw-loader": "4.0.2",
    "react": "17.0.2",
    "rimraf": "3.0.2",
    "sass": "1.49.0",
    "sass-loader": "12.4.0",
    "shelljs": "0.8.5",
    "stringify-object": "3.3.0",
    "stylelint": "14.3.0",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-recommended-scss": "5.0.2",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-processor-html": "1.0.0",
    "stylelint-scss": "4.1.0",
    "svgson": "5.2.1",
    "ts-node": "10.4.0",
    "tsconfig-replace-paths": "0.0.11",
    "tslib": "2.3.1",
    "typescript": "4.5.5",
    "vite": "2.7.13",
    "vite-plugin-eslint": "1.3.0",
    "vite-plugin-md": "0.11.7",
    "vitest": "0.6.0",
    "vue": "3.2.20",
    "vue-eslint-parser": "8.2.0",
    "vue-loader": "16.8.3",
    "vue-router": "4.0.12",
    "webpack": "5.70.0"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "files": [
    "**/*"
  ],
  "exports": {
    ".": {
      "require": "./inkline.cjs",
      "import": "./inkline.mjs",
      "types": "./inkline.d.ts"
    },
    "./*": "./*"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/inkline"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/inkline/inkline/issues"
  },
  "homepage": "http://inkline.io",
  "keywords": [
    "inkline",
    "vue",
    "vuejs",
    "ui",
    "ux",
    "dx",
    "components",
    "library",
    "framework",
    "typescript",
    "javascript",
    "scss",
    "responsive",
    "front end",
    "validation"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkline/inkline.git"
  }
}
